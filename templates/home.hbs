<h1>Your Place To Bid on FoodCart Food!</h1>
<h2>Here are our Vendors!</h2>
  <ul>
    {{#each}}
    <li {{action "showHide"}}>
      {{vendor}} - {{lot}}
      {{#if isShowing}}
        <ul>
          {{#each thing in item}}
            <li>{{#link-to "itemDetails" thing}}{{thing.description}}{{/link-to}} - {{thing.story}}</li>
          {{/each}}
        </ul>
      {{/if}}
    </li>
    {{/each}}
  </ul>
