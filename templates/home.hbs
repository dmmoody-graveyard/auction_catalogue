<h1>Home</h1>
<h2>Here are our Vendors!</h2>


  <ul>
    {{#each}}
    <li {{action "show"}}>
      {{vendor}} - {{lot}}
      {{#if isShowing}}
        <ul>
          {{#each thing in item}}
            <li {{action "hide"}}>{{#link-to "itemDetails" thing}}{{thing.description}}{{/link-to}} - {{thing.story}}</li>
          {{/each}}
        </ul>
      {{/if}}
    </li>
    {{/each}}
  </ul>
